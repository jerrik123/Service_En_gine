<check-list>
	<check mangoId="_queryMbrShipListByMbrid">
		<param-list>
			<sub-param name="mbrId">
				<rule-list>
					<rule key="required" />
					<rule key="number" />
					<rule key="min" value="0" />
					<rule key="max" value="999999" />
					<rule key="rangelength" value="[3,]" />
					<rule key="maxlength" value="6" />
					<rule key="minlength" value="4" />
					<rule key="range" value="[85231,123456]" />
				</rule-list>
			</sub-param>
			<sub-param name="loginName">
				<rule-list>
					<rule key="required" />
					<rule key="email|mobile" />
				</rule-list>
			</sub-param>
			<sub-param name="loginPwd">
				<rule-list>
					<rule key="required" />
					<rule key="rangelength" value="[6,12]" />
				</rule-list>
			</sub-param>
		</param-list>
	</check>
	<check mangoId="updateMobileNo">
		<param-list>
			<sub-param name="oldMobile">
				<rule-list>
					<rule key="required" />
					<rule key="mobile" />
				</rule-list>
			</sub-param>
			<sub-param name="newMobile">
				<rule-list>
					<rule key="required" />
					<rule key="mobile" />
				</rule-list>
			</sub-param>
		</param-list>
	</check>
	<check mangoId="cutPoint">
		<param-list>
			<sub-param name="mbrId">
				<rule-list>
					<rule key="required" />
					<rule key="number" />
					<rule key="min" value="1" />
				</rule-list>
			</sub-param>
			<sub-param name="points">
				<rule-list>
					<rule key="required" />
					<rule key="min" value="1" />
				</rule-list>
			</sub-param>
			<sub-param name="type">
				<rule-list>
					<rule key="required" />
					<rule key="range" value="[AB]"/>
				</rule-list>
			</sub-param>
			<sub-param name="signCode">
				<rule-list>
					<rule key="required" />
				</rule-list>
			</sub-param>
			<sub-param name="salesTransNO">
				<rule-list>
					<rule key="required" />
				</rule-list>
			</sub-param>
		</param-list>
	</check>
	<check mangoId="queryMobileNoByMbrId">
		<param-list>
			<sub-param name="mbrId">
				<rule-list>
					<rule key="required" />
					<rule key="number" />
					<rule key="min" value="1" />
				</rule-list>
			</sub-param>
		</param-list>
	</check>
	<check mangoId="queryMobileNoByLoginName">
		<param-list>
			<sub-param name="loginName">
				<rule-list>
					<rule key="required" />
				</rule-list>
			</sub-param>
		</param-list>
	</check>
	
	<check mangoId="addAwardPoints">
		<param-list>
			<sub-param name="mbrId">
				<rule-list>
					<rule key="required" />
					<rule key="number" />
					<rule key="min" value="1" />
				</rule-list>
			</sub-param>
			<sub-param name="order_src">
				<rule-list>
					<rule key="required" />
				</rule-list>
			</sub-param>
			<sub-param name="order_num">
				<rule-list>
					<rule key="required" />
				</rule-list>
			</sub-param>
			<sub-param name="sales_trans_code">
				<rule-list>
					<rule key="required" />
				</rule-list>
			</sub-param>
			<sub-param name="sales_trans_amount">
				<rule-list>
					<rule key="required" />
				</rule-list>
			</sub-param>
		</param-list>
	</check>
	
	<check mangoId="adminLogin">
		<param-list>
			<sub-param name="loginName">
				<rule-list>
					<rule key="required" />
				</rule-list>
			</sub-param>
			<sub-param name="loginPassword">
				<rule-list>
					<rule key="required" />
				</rule-list>
			</sub-param>
		</param-list>
	</check>
	
	<check mangoId="sendSms">
		<param-list>
			<sub-param name="mobileNo">
				<rule-list>
					<rule key="required" />
					<rule key="mobile" />
				</rule-list>
			</sub-param>
			<sub-param name="sendType">
				<rule-list>
					<rule key="required" />
					<rule key="range" value="[verify_code,comm_code]"/>
				</rule-list>
			</sub-param>
		</param-list>
	</check>
	
	<check mangoId="validateSms">
		<param-list>
			<sub-param name="mobileNo">
				<rule-list>
					<rule key="required" />
					<rule key="mobile" />
				</rule-list>
			</sub-param>
			<sub-param name="signCode">
				<rule-list>
					<rule key="required" />
					<rule key="number" />
				</rule-list>
			</sub-param>
		</param-list>
	</check>
</check-list>